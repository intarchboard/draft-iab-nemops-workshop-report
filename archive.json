      "reviews": [
        {
          "id": "PRR_kwDONe90rM66fTPt",
          "commit": {
            "abbreviatedOid": "2dc3095"
          },
          "author": "hardaker",
          "authorAssociation": "COLLABORATOR",
          "state": "COMMENTED",
